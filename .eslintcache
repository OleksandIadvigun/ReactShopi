             value={latitude}\r\n                                       onChange={(e) => {\r\n                                           setLatitude(e.target.value)\r\n                                       }}\r\n                                />\r\n                                <br/>\r\n                                <input className=\"form-control my\"\r\n                                       id='longitude'\r\n                                       type='number'\r\n                                       placeholder=\"Input longitude\"\r\n                                       name=\"longitude\"\r\n                                       value={longitude}\r\n                                       onChange={(e) => {\r\n                                           setLongitude(e.target.value)\r\n                                       }}\r\n                                />\r\n                                <br/>\r\n                                <input className=\"form-control my\"\r\n                                       id='areaSize'\r\n                                       type='number'\r\n                                       placeholder=\"Input area size\"\r\n                                       name=\"areaSize\"\r\n                                       value={areaSize}\r\n                                       onChange={(e) => {\r\n                                           setSize(e.target.value)\r\n                                       }}\r\n                                />\r\n                                {ErrorAreaSize ? <div className=\"error\">{ErrorAreaSize}\r\n                                    </div>\r\n                                    : null}\r\n\r\n                                <TransitionGroup>\r\n                                    {isLoadingInside ? SpinnerInside : <div></div>}\r\n                                    {!AddOrEdit ?\r\n                                        <div className={styles.buttonAdd}>\r\n                                            <button className={styles.navLink} onClick={addProduct}>Add</button>\r\n                                        </div> :\r\n                                        <CSSTransition\r\n                                            in={AddOrEdit}\r\n                                            timeout={1000}\r\n                                            classNames=\"example\"\r\n                                        >\r\n                                            <div className={styles.buttonAdd2}>\r\n                                                <button className={styles.navLinkCancel} onClick={cancel}>Cancel\r\n                                                </button>\r\n                                                <button className={styles.navLink} onClick={editProd}>Edit</button>\r\n                                            </div>\r\n                                        </CSSTransition>\r\n                                    }</TransitionGroup>\r\n                            </div>\r\n                        </form>\r\n                    </div>\r\n\r\n                </div>\r\n                <div>\r\n                    <TransitionGroup>\r\n                        {\r\n                            shops.map((item, index) =>\r\n                                <CSSTransition\r\n                                    key={item.id}\r\n                                    timeout={500}\r\n                                    classNames=\"example\"\r\n                                >\r\n                                    <Shop item={item}\r\n                                          edit={edit}\r\n                                          del={del}\r\n                                          key={item.id}\r\n                                          index={index}\r\n                                    />\r\n                                </CSSTransition>\r\n                            )\r\n                        }\r\n               