              </div>\r\n\r\n                            }\r\n\r\n\r\n                            {this.state.loader ? SpinnerInside : <div></div>}\r\n                            {this.state.forgot ?\r\n                                <div className=\"containerButtons\">\r\n                                    <div className=\"back\" onClick={() => {\r\n                                        this.setState({forgot: false});\r\n                                        this.setState({responseError: ''})\r\n                                    }}>\r\n                                        <div className=\"arrow-left\">\r\n                                            <span></span>\r\n                                            <span></span>\r\n                                            <span></span>\r\n                                        </div>\r\n                                    </div>\r\n                                    <button type=\"submit\" id=\"sub\" className=\"btn btn-primary w-50\"\r\n                                            onClick={this.onSubmit}>Reset password\r\n                                    </button>\r\n                                </div>\r\n                                :\r\n                                <button type=\"submit\" id=\"sub\" className=\"btn btn-primary w-50\"\r\n                                        onClick={this.onSubmit}>Log\r\n                                    in</button>\r\n                            }\r\n                        </form>\r\n                    </div>\r\n                </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default LogIn;\r\n","C:\\Users\\iadvi\\IdeaProjects\\reactShopi\\src\\components\\shops\\Shops.js",["209","210","211","212"],"import React, {useEffect, useState} from \"react\";\r\nimport axios from \"axios\";\r\nimport styles from \"./Shops.module.css\";\r\nimport Shop from \"../shop/Shop\";\r\nimport {CSSTransition, TransitionGroup} from \"react-transition-group\";\r\nimport '../